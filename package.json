{
  "name": "phaser-tutorial-1",
  "version": "1.0.0",
  "main": "js/index.js",
  "repository": "https://github.com/dalawwa/phaser-tutorial-1.git",
  "author": "Dany Alawwa <dany@goldwasserexchange.be>",
  "license": "MIT",
  "scripts": {
    "build": "yarn copy-assets && parcel build index.html --no-source-maps -d build",
    "clean": "rm -rf build",
    "copy-assets": "yarn clean && mkdir build && cp -R public/* build",
    "serve": "serve -s build",
    "start": "yarn build && yarn serve"
  },
  "dependencies": {
    "phaser": "^3.22.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "serve": "^11.3.0"
  }
}
